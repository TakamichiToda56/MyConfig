[user]
    name = TakamichiToda56
    email = 5toda6@gmail.com
[color]
    ui = auto
    diff = auto
    status = auto
    branch = auto
[alias]
    co = checkout
    br = branch
    st = status
    cm = commit -m
    df = diff
    gr = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
[core]
    quotepath = off
    editor = vim -c \"set fenc=utf-8\"
[filter "lfs"]
    clean = git lfs clean %f
    smudge = git lfs smudge %f
    required = true
